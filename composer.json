{
    "name": "ligadonasaude/framework",
    "type": "library",
    "description": "PHP framework for Portal ligado na saúde",
    "keywords": ["portal", "ligado", "saúde", "framework", "php"],
    "homepage": "https://github.com/ligadonasaude/framework",
    "support": {
        "issues": "https://github.com/ligadonasaude/framework/issues"
    },
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Ligado na saúde",
            "email": "contato@ligadonasaude.com.br",
            "homepage": "http://ligadonasaude.com.br",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "<6.0.0"
    },
    "autoload": {
        "psr-0": {
            "LG\\": "/"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "LG\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "master"
        }
    }
}